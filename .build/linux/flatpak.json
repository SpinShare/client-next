{
  "app-id" : "spinshare.client.next",
  "runtime" : "org.freedesktop.Platform",
  "runtime-version" : "23.08",
  "sdk" : "org.freedesktop.Sdk",
  "command" : "SpinShareClient",
  "modules" : [
    {
      "name" : "dotnet",
      "buildsystem" : "simple",
      "build-commands" : [
        "install --reinstall -D -t /app/bin Photino.Native.so",
        "install --reinstall -D -t /app/bin SpinShareClient",
        "install --reinstall -D -t /app/bin SpinShareClient.pdb"
      ],
      "sources" : [
        {
          "type" : "file",
          "path" : "../../build/ubuntu-latest/Photino.Native.so"
        },
        {
          "type" : "file",
          "path" : "../../build/ubuntu-latest/SpinShareClient"
        },
        {
          "type" : "file",
          "path" : "../../build/ubuntu-latest/SpinShareClient.pdb"
        }
      ]
    }
  ],
  "finish-args" : [
    "--share=ipc",
    "--socket=x11",
    "--socket=wayland",
    "--device=dri"
  ]
}